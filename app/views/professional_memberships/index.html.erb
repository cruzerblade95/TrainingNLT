<p id="notice"><%= notice %></p>

<h1>Professional Memberships</h1>

<table>
  <thead>
    <tr>
      <th>Association</th>
      <th>Institution</th>
      <th>Duration date</th>
      <th>Annual subs</th>
      <th>Personal detail</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @professional_memberships.each do |professional_membership| %>
      <tr>
        <td><%= professional_membership.association %></td>
        <td><%= professional_membership.institution %></td>
        <td><%= professional_membership.duration_date %></td>
        <td><%= professional_membership.annual_subs %></td>
        <td><%= professional_membership.personal_detail %></td>
        <td><%= link_to 'Show', professional_membership %></td>
        <td><%= link_to 'Edit', edit_professional_membership_path(professional_membership) %></td>
        <td><%= link_to 'Destroy', professional_membership, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Professional Membership', new_professional_membership_path %>
